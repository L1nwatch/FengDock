# Image pushed to GHCR (overridden by deployment pipeline)
GHCR_IMAGE=ghcr.io/your-github-username/fengdock:latest

# FastAPI scheduler interval (minutes)
LINK_CHECK_INTERVAL_MINUTES=30

# Domain used by Caddy for TLS provisioning
DOMAIN=example.com
# Optional global options for Caddy (e.g. "email admin@example.com")
CADDY_GLOBAL_OPTIONS=
